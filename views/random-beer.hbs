<div>
    {{#each this.randomBeer}}
    <h2>{{this.name}}</h2>
    <img src="{{this.image_url}}" alt="random-beer" width="100px">
    <h4>{{this.tagline}}</h4>
    <p>{{this.description}}</p>
    <h4>Food pairing:</h4>
    {{#each this.food_pairing}}
    <li>{{this}}</li>
        {{/each}}
    <p>Brewers Tips: <span>{{this.brewers_tips}}</span></p>

    {{/each}}
</div>